{
    "provider": {
        "name": "BusService"
    },
    "consumer": {
        "name": "BusServiceClient"
    },
    "interactions": [
        {
            "description": "A request for eta for bus number 613 to HammerSmith bus station",
            "request": {
                "method": "GET",
                "path": "/bus/HammerSmith/613"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "eta": 4,
                    "nr": 613,
                    "station": "HammerSmith"
                },
                "matchingRules": {
                    "body": {
                        "$.station": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.nr": {
                            "matchers": [
                                {
                                    "match": "type"
                                }
                            ],
                            "combine": "AND"
                        },
                        "$.eta": {
                            "matchers": [
                                {
                                    "match": "integer"
                                }
                            ],
                            "combine": "AND"
                        }
                    }
                }
            },
            "providerStates": [
                {
                    "name": "This is a bus with number 613 arriving to HammerSmith bus station"
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.0"
        }
    }
}